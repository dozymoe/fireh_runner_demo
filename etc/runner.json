{
    "modules": [
        "fireh_runner.django",
        "fireh_runner.pip",
        "fireh_runner.uwsgi_django",
        "fireh_runner.waf"
    ],
    "package_name": "fireh_runner_demo",
    "default_project": "blog",
    "default_variant": "development",
    "python_version": "3.4",
    "virtualenv_dir": ".virtualenv",

    "configuration": {
        "development": {
            "blog": {
                "worker_count": 1,
                "max_requests": 5000,
                "socket_path": "./tmp/sockets/blog.sock"
            }
        }
    }
}
